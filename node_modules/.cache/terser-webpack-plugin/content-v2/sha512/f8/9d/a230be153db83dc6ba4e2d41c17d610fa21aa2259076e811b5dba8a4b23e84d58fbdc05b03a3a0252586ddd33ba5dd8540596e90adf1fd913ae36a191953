-c\\\"><img src={verifyCode} alt=\\\"\\\" className=\\\"cm-img-04\\\"/><span className=\\\"cm-c-main\\\" onClick={()=>this.getVerify()}> {this.state.getCode === \\\"获取验证码\\\"?\\\"获取验证码\\\":this.state.getCode+\\\"秒重新获取\\\"}</span></div>\\r\\n        )\\r\\n    }\\r\\n    render() {\\r\\n        return<div>\\r\\n            <Title  that={this} title=\\\"忘记密码\\\"/>\\r\\n            <div className=\\\"cm-mlr-02 cm-mt-02\\\">\\r\\n                <List>\\r\\n                    <Input\\r\\n                        label=\\\"手机号\\\"\\r\\n                        maxLength={11}\\r\\n                        placeholder=\\\"请输入手机号\\\"\\r\\n                        defaultValue={this.state.mobile}\\r\\n                        onChange={(val)=>this.change({mobile:val})}\\r\\n                        className=\\\"cm-c-999\\\"\\r\\n                    />\\r\\n                </List>\\r\\n                <List>\\r\\n                    <Input\\r\\n                        label=\\\"验证码\\\"\\r\\n                        placeholder=\\\"请输入短信验证码\\\"\\r\\n                        defaultValue={this.state.verifyCode}\\r\\n                        onChange={(val)=>this.change({verifyCode:val})}\\r\\n                        renderRight = {this.renderRight()}\\r\\n                    />\\r\\n                </List>\\r\\n                <List>\\r\\n                    <Input\\r\\n                        label=\\\"新密码\\\"\\r\\n                        type=\\\"password\\\"\\r\\n                        placeholder=\\\"请输入新密码\\\"\\r\\n                        onChange={(val)=>this.change({password:val})}\\r\\n                        maxLength={16}\\r\\n                    />\\r\\n                </List>\\r\\n                <List>\\r\\n                    <Input\\r\\n                        label=\\\"确认密码\\\"\\r\\n                        type=\\\"password\\\"\\r\\n                        placeholder=\\\"请输入确认密码\\\"\\r\\n                        value={this.state.confirmPwd}\\r\\n                     