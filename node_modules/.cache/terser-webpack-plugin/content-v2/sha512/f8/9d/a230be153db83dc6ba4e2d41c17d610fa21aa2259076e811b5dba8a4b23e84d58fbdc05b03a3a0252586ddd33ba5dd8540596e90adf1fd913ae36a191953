号直接从父组件传递过来\\r\\n            verifyCode:\\\"\\\",//初始化验证码\\r\\n        };\\r\\n        this.type = props.type;\\r\\n    }\\r\\n    change(state){\\r\\n        this.setState(state)\\r\\n    }\\r\\n    //验证手机号\\r\\n    getVerify(){\\r\\n        if(!isDefine(this.state.mobile)){\\r\\n            Toast.info(\\\"请输入手机号\\\");\\r\\n            return;\\r\\n        }\\r\\n        getData({\\r\\n            method:'post',\\r\\n            url:'getVerifyCode',\\r\\n            data:{\\r\\n                mobile:this.state.mobile,\\r\\n                type:this.type\\r\\n            },\\r\\n            successCB:(res)=> {\\r\\n                this.setState({\\r\\n                    getCode:60\\r\\n                },()=>{\\r\\n                    this.timer = setInterval(()=>{\\r\\n                        //验证码过了60秒，消除定时器\\r\\n                        if(this.state.getCode === 1){\\r\\n                            clearInterval(this.timer);\\r\\n                            this.setState({\\r\\n                                getCode:\\\"获取验证码\\\"\\r\\n                            })\\r\\n                        }else {\\r\\n                            //验证码60秒倒计时\\r\\n                            this.setState({\\r\\n                                getCode:--this.state.getCode\\r\\n                            })\\r\\n                        }\\r\\n                    },1000)\\r\\n                })\\r\\n            }\\r\\n        })\\r\\n    }\\r\\n    renderRight(){\\r\\n        return(\\r\\n            <div className=\\\"cm-flex cm-ai-c\\\"><img src={verifyCode} alt=\\\"\\\" className=\\\"cm-img-04\\\"/><span className=\\\"cm-c-main\\\" onClick={()=>this.getVerify()}> {this.state.getCode === \\\"获取验证码\\\"?\\\"获取验证码\\\":this.state.getCode+\\\"秒重新获取\\\"}</span></div>\\r\\n        )\\r\\n    }\\r\\n    //点击下一步或者完成的回调\\r\\n    submit(){\\r\\n        if(this.props.submit){\\r\\n            this.props.submit({mobile:this.state.mobile,verifyCode:this.state.verifyCode});\\r\\n        }\